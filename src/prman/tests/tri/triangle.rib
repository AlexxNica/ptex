##RenderMan RIB-Structure 1.1
version 3.04

IfBegin "1==1"
# normal shading rate
PixelFilter "mitchell" 4 4
PixelSamples 5 5
ShadingInterpolation "smooth"
ShadingRate 1
Hider "hidden" "jitter" [1] "depthfilter" ["min"]

Else
# large shading rate w/ subpixels exposed
PixelFilter "box" 1 1
PixelSamples 1 1
#Hider "stochastic" "subpixel" [1]
ShadingInterpolation "constant"
ShadingRate 100
Hider "hidden" "jitter" [0] "depthfilter" ["min"]
IfEnd

Declare "originOffset" "constant float[2]"
Display "triangle.tif" "it" "rgba"
Option "limits" "eyesplits" [3]
Option "limits" "bucketsize" [16 16]
Option "limits" "gridsize" [256]
Declare "enable_lerp" "uniform float"
Option "texture" "enable lerp" [1]
Option "texture" "enable gaussian" [1]
Option "limits" "texturememory" [32768]
Option "searchpath" "shader" [".:..:../.."]
Option "searchpath" "texture" ["."]
Option "limits" "othreshold" [0.99599999 0.99599999 0.99599999]
Option "limits" "zthreshold" [1 1 1]
Option "shadow" "bias0" [0.22499999] "bias1" [0.22499999]
GeometricApproximation "motionfactor" 0
Option "trace" "int maxdepth" [10]
Option "trace" "float specularthreshold" [10]
Exposure 1 1
Format 512 512 1
Matte 0
Attribute "displacementbound" "sphere" [0] "coordinatesystem" ["object"]
Attribute "dice" "binary" [1]
Sides 2
Attribute "sides" "backfacetolerance" [0]
Attribute "derivatives" "centered" [1]
Attribute "derivatives" "extrapolate" [1]
Attribute "visibility" "int camera" [1]
Attribute "visibility" "int trace" [0]
Attribute "trace" "int maxdiffusedepth" [1]
Attribute "trace" "int maxspeculardepth" [2]
Attribute "trace" "int displacements" [0]
Attribute "trace" "float bias" [0.0099999998]
Attribute "trace" "int samplemotion" [0]
Attribute "user" "string shaderSurf" ["true"]
Attribute "user" "string shaderDisp" ["false"]
Attribute "user" "string shaderAtm" ["false"]
Attribute "user" "string isShadowPass" ["false"]
Attribute "user" "uniform float passID" [0]
PixelSamples 5 5
Shutter 0 0
Option "shutter" "int clampmotion" [1]
Quantize "rgba" 255 0 255 0.5
Option "shutter" "float offset" [1]
Option "statistics" "endofframe" [1]
ErrorHandler "printonce"
FrameBegin 1
ScreenWindow -1 1 -1 1
Clipping 0.1 1000
Projection "perspective" "fov" [54.432129]
